{
  "name": "@weccoframework/storybook-renderer",
  "version": "0.1.0",
  "description": "Storybook for wecco: Develop wecco view functions and components in isolation with Hot Reloading.",
  "license": "Apache 2.0",
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preview": {
      "require": "./dist/previewAnnotations.js",
      "import": "./dist/previewAnnotations.mjs",
      "types": "./dist/previewAnnotations.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "./preview.js"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build": "rollup -c",
    "watch": "rollup -c --watch"
  },
  "dependencies": {
    "@weccoframework/core": "^0.24.0",
    "@storybook/addons": "^7.0.12",
    "@storybook/builder-vite": "^7.0.12",
    "@storybook/channel-postmessage": "^7.0.12",
    "@storybook/channel-websocket": "^7.0.12",
    "@storybook/client-api": "^7.0.12",
    "@storybook/core-server": "^7.0.12",
    "@storybook/html": "^7.0.12",
    "@storybook/node-logger": "^7.0.12",
    "@storybook/preview-web": "^7.0.12",
    "magic-string": "^0.30.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "typescript": "^5.0.4",
    "rollup": "^3.22.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.34.1"
  },
  "engines": {
    "node": "^14.18 || >=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
